{
    "name":"colorPicker",
    "description":"Pick and get to know any color",
    "version":"0.1.0",
    "manifest_version":3,
    "background": {
        "service_worker":"background.js"
    },
    "permissions": ["activeTab","scripting"],
    "action": {
        "default_popup":"popup.html",
        "default_icon":"icon.png"
    }
}